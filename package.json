{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a",
  "engines": {
    "node": ">=22.20.0"
  },
  "scripts": {
    "test": "vitest",
    "test:cli": "pnpm --filter @digdir/designsystemet test --verbose",
    "test:storybook": "pnpm --filter @web/storybook test-storybook",
    "test:coverage": "vitest run --coverage",
    "storybook": "pnpm --filter @web/storybook dev",
    "www": "pnpm --filter @web/www dev",
    "themebuilder": "pnpm --filter @web/themebuilder dev",
    "figma-plugin": "pnpm --filter @plugin/figma dev",
    "build": "pnpm build:cli && pnpm --filter @digdir/* build",
    "build:all": "pnpm build:cli && pnpm -r build",
    "build:figma-plugin": "pnpm --filter @plugin/figma build",
    "build:react": "pnpm --filter @digdir/designsystemet-react build",
    "build:css": "pnpm --filter @digdir/designsystemet-css build",
    "build:cli": "pnpm --filter @digdir/designsystemet build",
    "build:storybook": "pnpm --filter @web/storybook build",
    "build:www": "pnpm --filter @web/www build",
    "build:themebuilder": "pnpm --filter @web/themebuilder build",
    "types": "pnpm -r types",
    "types:react": "pnpm --filter @digdir/designsystemet-react types",
    "version-packages": "changeset version",
    "publish-packages": "pnpm build && changeset publish",
    "chromatic": "pnpm --filter @web/storybook chromatic",
    "update:template": "pnpm --filter @digdir/designsystemet update:template",
    "update:theme-digdir": "pnpm --filter @digdir/designsystemet update:theme-digdir",
    "update:theme": "pnpm build:cli && pnpm --filter @digdir/designsystemet-theme update:theme"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@svitejs/changesets-changelog-github-compact": "^1.2.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/node": "^22.18.6",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "chromatic": "^13.2.1",
    "just-pnpm": "^1.0.2",
    "tsconfck": "^3.1.6",
    "typescript": "^5.9.2",
    "typescript-plugin-css-modules": "^5.2.0",
    "vite": "^7.1.7",
    "vitest": "3.2.4"
  }
}
